%!PS

0 setgray
1 setlinewidth
0 setlinecap
/Outline 0 def

/inch { 72 mul } def

/centerShow
{
   dup
   gsave
      dup stringwidth pop 2 div neg
   grestore
   gsave
      exch true charpath pathbbox exch
      pop sub 2 div exch pop
   grestore
   rmoveto
   Outline 1 eq
     { true charpath gsave 0.75 setgray fill grestore 0.5 setlinewidth stroke }
     { show } ifelse
} def

/SuBox
{
    gsave
    0.5 setlinewidth
    2 setlinecap
    0 1 9 { gsave 3 div inch 0 moveto 0 3 inch rlineto stroke grestore } for
    0 1 9 { gsave 0 exch 3 div inch moveto 3 inch 0 rlineto stroke grestore } for
    2 setlinewidth
    0 1 3 { gsave inch 0 moveto 0 3 inch rlineto stroke grestore } for
    0 1 3 { gsave 0 exch inch moveto 3 inch 0 rlineto stroke grestore } for
    grestore
} def

/SuMoveto
{
    exch 2 mul -1 add 6 div inch  exch 2 mul -1 add 6 div -1 mul 3 add inch moveto
} def

